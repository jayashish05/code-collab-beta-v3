{
  "version": 2,
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/css/ios-style.css",
      "dest": "/public/css/ios-style.css",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0",
        "content-type": "text/css; charset=utf-8"
      }
    },
    {
      "src": "/css/auth.css",
      "dest": "/public/css/auth.css",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0",
        "content-type": "text/css; charset=utf-8"
      }
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0",
        "content-type": "text/css; charset=utf-8"
      }
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0",
        "content-type": "application/javascript; charset=utf-8"
      }
    },
    {
      "src": "/img/(.*)",
      "dest": "/public/img/$1",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0"
      }
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
